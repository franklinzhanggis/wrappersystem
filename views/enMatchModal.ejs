<!DOCTYPE html>
<html>

<head>
    <title>Environmental Matching</title>
    <link href="/css/uploadfile.css" rel="stylesheet" />
    <link rel="stylesheet" href="/js/data-tables/DT_bootstrap.css" />
    <link rel="stylesheet" type="text/css" href="/js/gritter/css/jquery.gritter.css" />
    <link href="/css/prettify.css" rel="stylesheet" />
    <link href="/css/bootstrap-modal-bs3patch.css" rel="stylesheet" />
    <link href="/css/bootstrap-modal.css" rel="stylesheet" />
    <link rel="stylesheet" type="text/css" href="/js/webix/webix.css" />
    <!-- <link rel="stylesheet" href="/css/enviroment/enviro.css"> -->
    <%- include headInclude.ejs %>

        <style>
            .matchList-matched {
                background-color: rgba(0, 67, 255, 0.2) !important
            }

            .matchList-unmatched {
                background-color: rgba(255, 0, 0, 0.2) !important
            }

            .webix_cell[aria-level='2']>.webix_tree_checkbox {
                display: none
            }

            .webix_cell[aria-level='3']>.webix_tree_checkbox {
                display: none
            }

            .webix_cell[aria-level='4']>.webix_tree_checkbox {
                display: none
            }

            .webix_cell[aria-level='5']>.webix_tree_checkbox {
                display: none
            }

            .webix_cell[aria-level='6']>.webix_tree_checkbox {
                display: none
            }

            .tabletree-header {
                height: 35px;
            }

            .tabletree-operate {
                text-align: center;
            }

            .tabletree-btn {
                margin: 0 5px;
            }

            .tabletree-delbtn {
                background-color: #c5555f;
                border-color: #ac2e41;
            }

            .tabletree-delbtn:hover {
                background-color: #c53c41;
                border-color: #ac2d40;
            }

            .tabletree-editbtn {
                background-color: #a9d86e;
                border-color: #90d85f;
            }

            .tabletree-editbtn:hover {
                background-color: #94d86c;
                border-color: #90d85f;
            }

            input[type=checkbox] {
                margin: 0 !important;
            }

            .paging1 {
                width: 100px;
                float: left;
                text-align: center;
            }

            .webix_checked {
                color: #47a447;
                font-weight: bold;
                cursor: pointer;
            }

            .webix_notchecked {
                color: #ed9c28;
                font-weight: bold;
                cursor: pointer;
            }

            .panel-heading {
                text-transform: none
            }
        </style>
</head>

<body class="sticky-header">
    <section>
        <!-- 左导航栏 -->
        <%- include header.ejs %>
            <div class="main-content">
                <!-- 上导航栏 -->
                <%- include nav.ejs %>
                    <section class="panel">
                        <header class="panel-heading">Environment - Matching</header>
                        <div class="panel-body">
                            <section id="enMatchModal" style="background-color: #FFFFFF;width:100%" oid="<%=modelSer._id %>" pid="<%=modelSer.ms_model.p_id %>"
                                place="<%=place %>">
                            </section>
                        </div>
                    </section>

                    <!-- <section id="enMatchModal" style="background-color: #FFFFFF;width:800px" oid="<%=modelSer._id %>" pid="<%=modelSer.ms_model.p_id %>"
                        place="<%=place %>">
                    </section> -->

                    <%- include footer.ejs %>
            </div>
    </section>
    <%- include footerInclude.ejs %>

        <script type="text/javascript" src="/js/prettify.js"></script>
        <script type="text/javascript" src="/js/bootstrap-modalmanager.js"></script>
        <script type="text/javascript" src="/js/bootstrap-modal.js"></script>
        <script type="text/javascript" src="/js/webix/webix.js"></script>
        <script type="text/javascript" src="/js/gritter/js/jquery.gritter.js"></script>
        <script type="text/javascript" src="/js/output/bundle.js"></script>
</body>

</html>