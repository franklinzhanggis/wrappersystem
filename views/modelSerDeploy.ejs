<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>部署模型服务</title>
    <link rel="stylesheet" href="/css/main1.css">
    <link rel="stylesheet" href="/css/bootstrap.css">
    <link rel="stylesheet" media="screen" href="/css/deployModel.css" />
    <link rel="stylesheet" href="/css/uploadfile.css" />
    <script src="/js/jquery-1.9.1.min.js" type="text/javascript" ></script>
    <script src="/js/jquery.uploadfile.min.js" type="text/javascript"></script>
</head>
<body>

<%- include header.ejs %>

<!--start 主体body-->
<div class="container">
    <div class="title">部署模型基本信息</div>
    <div id="msform">
        <!-- fieldsets -->
        <form method="post" action="/modelser" >
            <fieldset>
                <div >
                    <label >模型名称：<%= model.m_name %></label>
                </div>
                <div >
                    <label >模型描述：<%= model.m_des %></label>
                </div>
                <div >
                    <input name="m_id" type="hidden" value="<%= model._id %>" />
                    <label>模型服务版本号</label>
                    <select name="mv_num" class="hor_inline" id="mv_num">
                            <option value="0">未知</option>
                            <% model.m_visions.forEach(function(item)
                            {
                                %><option value="<%= item.mv_num %>"><%= item.mv_num %></option><%
                            }); %>
                    </select>
                    <small>*这里需要模型版本号</small>
                </div>
                <div >
                    <label >模型服务描述</label>
                    <input name="ms_des" type="text"  class="hor_inline" id="ms_des"
                           placeholder="请输入描述信息">
                    <small>*这里需要填入模型服务描述</small>
                </div>
                <div >
                    <label>模型服务参数</label>
                    <input name="ms_xml" type="text"  class="hor_inline" id="ms_xml"
                           placeholder="请输入参数">
                    <small>*这里需要填入模型参数</small>
                </div>

                <hr>
                <input type="submit" name="next" id="btnNext1" class="next action-button"
                       value="下一步" />
            </fieldset>
        </form>
    </div>

    <script src="/js/jquery.js" type="text/javascript"></script>
    <script src="/js/bootstrap.js"></script>
    <!--<script src="js/jquery.easing.1.3.js" type="text/javascript"></script> -->
    <script src="/js/deployModel.js" type="text/javascript"></script>

</div><!-- // container end -->
<!--end 主体body-->

<hr/>

<%- include footer.ejs %>

</body>
</html>